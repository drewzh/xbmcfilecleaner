msgstr "[B]Hoeveel informatie wil je krijgen?[/B]"

msgctxt "#32502"
msgid "Show notifications"
msgstr "Toon meldingen"

msgctxt "#32503"
msgid "Do not show notifications during video playback"
msgstr "Toon geen meldingen als er video afgespeeld wordt"

msgctxt "#32504"
msgid "Enable debug messages"
msgstr "Toon debuginformatie"

